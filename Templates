import pandas as pd
import re

def fullfill_credit_union_info(df, col="financial_inst_name"):
    # Define patterns and replacements
    patterns = {
        r"\bCU\b": "CREDIT UNION",
        r"\bC U\b": "CREDIT UNION",
        r"\bCREDIT U\b": "CREDIT UNION",
        r"\bCREDITS\b": "CREDIT",
        r"\bCR UN\b": "CREDIT UNION",
        r"\bCR UNION\b": "CREDIT UNION",
        r"\bCR UNION\b": "CREDIT UNION",
        r"\bCRED UN\b": "CREDIT UNION",
        r"\bCRED UNION\b": "CREDIT UNION",
        r"\bCR UN\b": "CREDIT UNION",
        r"\bCR UN\b": "CREDIT UNION",
        r"\bCRED UNIONS\b": "CREDIT UNIONS",
        r"\bFED CRED UNION\b": "FEDERAL CREDIT UNION",
        r"\bFED CRED UN\b": "FEDERAL CREDIT UNION",
        r"\bFEDERAL CRED UNION\b": "FEDERAL CREDIT UNION",
        r"\bFEDERAL CRED UN\b": "FEDERAL CREDIT UNION",
        r"\bFED CREDIT UN\b": "FEDERAL CREDIT UNION",
        r"\bFEDERAL CU\b": "FEDERAL CREDIT UNION",
        r"\bFEDERAL C U\b": "FEDERAL CREDIT UNION",
        r"\bFED CU\b": "FEDERAL CREDIT UNION",
        r"\bFED C U\b": "FEDERAL CREDIT UNION",
        r"\bFEDERALCR\b": "FEDERAL CREDIT",
        r"\bFEDCR\b": "FEDERAL CREDIT",
        # Add more patterns as necessary
    }
    
    # Function to apply the replacements
    def replace_patterns(text, patterns):
        for pat, repl in patterns.items():
            text = re.sub(pat, repl, text, flags=re.IGNORECASE)
        return text
    
    # Apply the function across the specified column
    df[col] = df[col].apply(lambda x: replace_patterns(x, patterns))
    
    return df

# Assume 'df' is your dataframe and 'financial_inst_name' is the column with the bank names
# df = fullfill_credit_union_info(df, 'financial_inst_name')
