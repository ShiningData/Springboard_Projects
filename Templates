import pandas as pd
import random
import numpy as np

# Create a list of customer IDs and names
customer_ids = ['Cust1', 'Cust2', 'Cust3', 'Cust4', 'Cust5']
customer_names = ['Alice', 'Bob', 'Charlie', 'David', 'Eve']

# Create a dictionary to store the data
data = {
    'cust_id': random.choices(customer_ids, k=100),  # Generating 100 random customer IDs
    'cust_name': random.choices(customer_names, k=100),  # Generating 100 random customer names
}

# Add columns for each month from January to December with random float values or np.nan
months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December']

for month in months:
    values = [random.uniform(0, 100) if random.random() > 0.2 else np.nan for _ in range(100)]  # 20% chance of np.nan
    data[month] = values

# Create the DataFrame
df = pd.DataFrame(data)

# Display the DataFrame
print(df)
