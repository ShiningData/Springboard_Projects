import itertools
import pandas as pd

# Define the possible values for each column
address_mtch = ["Y", "N", "C", "U"]
city_match = ["Y", "N", "C", "U"]
state_mtch = ["Y", "N", "U"]
zip_match = ["Y", "N", "C", "U"]

# Generate all possible combinations
combinations = list(itertools.product(address_mtch, city_match, state_mtch, zip_match))

# Create a DataFrame
df_combinations = pd.DataFrame(combinations, columns=["AddressMtch", "CityMatch", "StateMtch", "ZipMatch"])

# Export to CSV if needed
df_combinations.to_csv('possible_combinations.csv', index=False)

# Print the first few rows of the DataFrame
print(df_combinations.head())

==========

import itertools
import pandas as pd

# Define the possible values for each column
id_type_mtch = ["Y", "N", "U"]
id_no_match = ["Y", "N", "C", "U"]
id_state_mtch = ["Y", "N", "U"]

# Generate all possible combinations
combinations = list(itertools.product(id_type_mtch, id_no_match, id_state_mtch))

# Create a DataFrame
df_combinations = pd.DataFrame(combinations, columns=["IDTypeMtch", "IDNoMatch", "IDStateMtch"])

# Export to CSV if needed
df_combinations.to_csv('possible_id_combinations.csv', index=False)

# Print the first few rows of the DataFrame
print(df_combinations.head())
