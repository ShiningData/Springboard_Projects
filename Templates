def validate_model_prediction(model_prediction, total_count, NameMtch, FNameMtch, LNameMtch, BusNameMtch, AddressMtch, CityMtch, StateMtch, ZipMtch, HmPhoneMtch, WkPhoneMtch, SSNMtch, DOBMtch, OverallMtchScore):

    def match_conditions(features_to_check, *args):
        """Check if any of the given features are 'N' or 'C'."""
        return any(feature in ['N', 'C'] for feature in features_to_check) and all(feature not in ['N', 'C'] for feature in args)

    def case_crc3100():
        if match_conditions([NameMtch, BusNameMtch], SSNMtch, DOBMtch, AddressMtch, CityMtch, StateMtch, ZipMtch, HmPhoneMtch, WkPhoneMtch):
            return 'CRC3100'
        return None

    def case_crc3200():
        if match_conditions([SSNMtch], NameMtch, DOBMtch, AddressMtch, CityMtch, StateMtch, ZipMtch, HmPhoneMtch, WkPhoneMtch):
            return 'CRC3200'
        return None

    def case_crc3300():
        if match_conditions([DOBMtch], NameMtch, SSNMtch, AddressMtch, CityMtch, StateMtch, ZipMtch, HmPhoneMtch, WkPhoneMtch):
            return 'CRC3300'
        return None

    def case_crc2100():
        if match_conditions([AddressMtch, CityMtch, StateMtch, ZipMtch], NameMtch, SSNMtch, DOBMtch, HmPhoneMtch, WkPhoneMtch):
            return 'CRC2100'
        return None

    def case_crc2200():
        if match_conditions([HmPhoneMtch, WkPhoneMtch], NameMtch, SSNMtch, DOBMtch, AddressMtch, CityMtch, StateMtch, ZipMtch):
            return 'CRC2200'
        return None

    def case_crc4100():
        if match_conditions([NameMtch, BusNameMtch], SSNMtch, DOBMtch, AddressMtch, CityMtch, StateMtch, ZipMtch, HmPhoneMtch, WkPhoneMtch) or \
           match_conditions([SSNMtch], NameMtch, DOBMtch, AddressMtch, CityMtch, StateMtch, ZipMtch, HmPhoneMtch, WkPhoneMtch):
            return 'CRC4100'
        return None

    def case_crc3400():
        if sum([DOBMtch in ['N', 'C'], match_conditions([AddressMtch, CityMtch, StateMtch, ZipMtch]), match_conditions([HmPhoneMtch, WkPhoneMtch])]) >= 2:
            return 'CRC3400'
        return None

    def resolve_cases_for_total_count_1():
        return case_crc3100() or case_crc3200() or case_crc3300() or case_crc2100() or case_crc2200() or model_prediction

    def resolve_cases_for_total_count_greater_than_1():
        return case_crc4100() or case_crc3400() or model_prediction

    # Main function logic based on model_prediction and total_count
    if model_prediction == "CRC1100":
        if total_count == 0:
            return 'CRC1100'
        elif total_count == 1:
            return resolve_cases_for_total_count_1()
        elif total_count > 1:
            return resolve_cases_for_total_count_greater_than_1()

    elif model_prediction == "CRC3400" or model_prediction == "CRC4100":
        if total_count == 1:
            return resolve_cases_for_total_count_1()
        elif total_count > 1:
            return resolve_cases_for_total_count_greater_than_1()

    # Default fallback if no condition matches
    return model_prediction
